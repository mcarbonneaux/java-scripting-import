            <file name="ScriptEnvironmentAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/org-chuk-lee-scriptengine-environment-ScriptEnvironmentAction.instance"/>
            </file>
        </folder>
        <attr name="Scripts/Help" boolvalue="true"/>
<!--        
        <folder name="Window">
            <file name="ScriptEnvironmentAction.shadow">
